{
  "name": "photos",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently --names \"CLIENT,SERVER\" -c \"blue,red\" --kill-others \"npm run serve\" \"npm run start:server\"",
    "dev:install": "concurrently \"npm ci\" \"npm ci --prefix server\"",
    "kill:process": "npx kill-port 3000 && npx kill-port 5173",
    "serve": "vite",
    "start:server": "npm run dev --prefix server",
    "build": "run-p type-check build-only && npm run build --prefix server",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@types/axios": "^0.14.0",
    "axios": "^1.5.0",
    "pinia": "^2.1.6",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.2",
    "@tailwindcss/vite": "^4.1.11",
    "@tsconfig/node18": "^18.2.0",
    "@types/concurrently": "^7.0.0",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^18.17.5",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitejs/plugin-vue-jsx": "^5.0.1",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "concurrently": "^8.2.1",
    "cypress": "^12.17.4",
    "eslint": "^8.46.0",
    "eslint-plugin-cypress": "^2.14.0",
    "eslint-plugin-vue": "^9.16.1",
    "jsdom": "^22.1.0",
    "nodemon": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.0",
    "start-server-and-test": "^2.0.0",
    "tailwindcss": "^4.1.11",
    "typescript": "~5.1.6",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-node-polyfills": "^0.24.0",
    "vitest": "^3.2.4",
    "vue-tsc": "^1.8.8"
  }
}
